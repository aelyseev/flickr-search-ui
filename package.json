{
  "name": "flickr-search-ui",
  "version": "0.0.1",
  "description": "Missing Flickr's search UI application",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "test": "cross-env NODE_ENV=test jest --no-cache",
    "build": "cross-env NODE_ENV=production webpack",
    "build:analyze-bundle": "cross-env NODE_ENV=production webpack",
    "deploy": "cross-env NODE_ENV=production webpack",
    "lint": "eslint \"app/**/*.js\" \"app/**/*.jsx\" --fix --cache --cache-location \"./.cache/\"",
    "lint:fix": "yarn lint --fix",
    "stylelint": "stylelint \"app/**/*.scss\"",
    "stylelint:fix": "stylefmt --recursive 'app/**/*.scss'",
    "test:watch": "cross-env NODE_ENV=development jest --watch"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "es6-promise": "^4.1.1",
    "final-form": "^4.6.1",
    "final-form-focus": "^1.0.0",
    "lodash": "^4.17.2",
    "moment": "^2.21.0",
    "nanoid": "^1.0.2",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-final-form": "^3.4.2",
    "react-infinite-scroller": "^1.1.4",
    "react-redux": "^5.0.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.2.1",
    "redux-observable": "^0.18.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.0",
    "rxjs": "5.x.x",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "~1.2.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.0",
    "clean-webpack-plugin": "^0.1.14",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.2.3",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "deep-freeze-strict": "^1.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.11",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.2.0",
    "invariant": "^2.2.2",
    "jest": "^22.4.4",
    "jest-cli": "^22.4.3",
    "json-loader": "^0.5.7",
    "lodash-webpack-plugin": "^0.11.3",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^4.1.3",
    "react-test-renderer": "^16.3.2",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylefmt": "^6.0.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "url-loader": "^1.0.1",
    "webpack": "^3.1.0",
    "webpack-bundle-analyzer": "^2.12.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-merge": "^4.1.0",
    "yaml-loader": "^0.5.0"
  },
  "babel": {
    "babelrc": false,
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              ">1%"
            ]
          },
          "loose": true,
          "modules": false
        }
      ],
      "stage-0",
      "react"
    ],
    "plugins": [
      "lodash",
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "prettier": {
    "printWidth": 140,
    "semi": true,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "arrowParens": "always"
  },
  "engines": {
    "node": "^8.7.0",
    "npm": "^3.10.0"
  },
  "jest": {
    "verbose": true,
    "globals": {
      "DEBUG": false
    },
    "roots": [
      "<rootDir>/app"
    ],
    "modulePaths": [
      "<rootDir>/app"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|sass|scss)$": "<rootDir>/.fixtures/style-mock.js",
      "^.+\\.path$": "<rootDir>/__mocks__/path-mock.js"
    },
    "testRegex": ".*spec.jsx?$",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/__mocks__/tests-setup.js"
    ]
  },
  "author": "Aleksey Elyseev",
  "license": "UNLICENSED"
}
